": [491.7929292929293, 205.87559366925063, 415.8289886363637, 114.51126518087858], "id_box_line": 41, "box_line": [491.7929292929293, 222.4292112403101, 414.1500420875421, 15.189559754521952], "text": "variable resistance signal from the tank unit. They convert ", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DRAYDX+Times-Roman", "size": 1}}, {"id_box": 539711, "box": [491.7929292929293, 205.87559366925063, 415.8289886363637, 114.51126518087858], "id_box_line": 42, "box_line": [491.7929292929293, 238.9828288113696, 414.60746127946135, 15.189559754521952], "text": "the variable resistance into a digital display in the cockpit ", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DRAYDX+Times-Roman", "size": 1}}, {"id_box": 539711, "box": [491.7929292929293, 205.87559366925063, 415.8289886363637, 114.51126518087858], "id_box_line": 43, "box_line": [491.7929292929293, 255.53644638242895, 111.15374368686861, 15.189559754521952], "text": "instrument head.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DRAYDX+Times-Roman", "size": 1}}, {"id_box": 539711, "box": [491.7929292929293, 205.87559366925063, 415.8289886363637, 114.51126518087858], "id_box_line": 44, "box_line": [602.8863636363636, 255.70853779069773, 107.55595917508413, 15.017468346253168], "text": " [Figure 14-70] ", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/RUBERV+Times-Italic", "size": 1}}, {"id_box": 539711, "box": [491.7929292929293, 205.87559366925063, 415.8289886363637, 114.51126518087858], "id_box_line": 45, "box_line": [710.4078282828283, 255.53644638242895, 193.49886153198645, 15.189559754521952], "text": "Fully digital instrumentation ", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DRAYDX+Times-Roman", "size": 1}}, {"id_box": 539711, "box": [491.7929292929293, 205.87559366925