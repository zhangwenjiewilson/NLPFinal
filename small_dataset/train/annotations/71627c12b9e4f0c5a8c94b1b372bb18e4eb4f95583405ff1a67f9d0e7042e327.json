ords": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DFIMCM+AkzidenzGroteskBE-Regular", "size": 1}}, {"id_box": 127555, "box": [543.0776258169934, 141.31770561941255, 422.15685171568634, 340.84135185185175], "id_box_line": 51, "box_line": [543.0783459967321, 342.91477107279695, 421.8765510620915, 10.956896551724185], "text": "channel wide enough for the tanker to reach the station. Without fuel,", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DFIMCM+AkzidenzGroteskBE-Regular", "size": 1}}, {"id_box": 127555, "box": [543.0776258169934, 141.31770561941255, 422.15685171568634, 340.84135185185175], "id_box_line": 52, "box_line": [543.0783459967321, 361.24220306513405, 421.90507352941177, 10.956883461047255], "text": "two base stations would not have been fully operational the following", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DFIMCM+AkzidenzGroteskBE-Regular", "size": 1}}, {"id_box": 127555, "box": [543.0776258169934, 141.31770561941255, 422.15685171568634, 340.84135185185175], "id_box_line": 53, "box_line": [543.0783459967321, 379.569661238825, 422.0376535947712, 10.956896551724185], "text": "season \u2014 jeopardizing the customer\u2019s mission of conducting sci-", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DFIMCM+AkzidenzGroteskBE-Regular", "size": 1}}, {"id_box": 127555, "box": [543.0776258169934, 141.31770561941255, 422.15685171568634, 340.84135185185175], "id_box_line": 54, "box_line": [543.0783459967321, 397.8971325031928, 421.96231944444435, 10.956883461047369], "text": "ence on the southernmost continent. RTSC successfully pumped", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/DFIMCM+AkzidenzGroteskBE-Regular", "size": 1}}, {"id_box": 127555, "box": [543.0776258169934, 141.31770561941255, 422.15685171568634, 340.84135185185175], "id_box_line": 55, "box_