[], "font": {"color": [0, 0, 0, 255], "name": "/FGPLEN+ZurichCnBT-Bold", "size": 1}}, {"id_box": 688999, "box": [108.3826466146247, 575.257541837477, 260.3088747574294, 16.72801386380803], "id_box_line": 20, "box_line": [157.72224088552005, 575.257541837477, 210.96928048653407, 16.72801386380803], "text": "Forming adverbs", "category": "Section-header", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLEN+ZurichCnBT-Bold", "size": 1}}, {"id_box": 689000, "box": [108.3826466146247, 661.8131002051039, 376.81004795398155, 16.727997213385265], "id_box_line": 23, "box_line": [108.3826466146247, 661.8131002051039, 20.872774249963683, 16.727997213385265], "text": "4.", "category": "Section-header", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLEN+ZurichCnBT-Bold", "size": 1}}, {"id_box": 689000, "box": [108.3826466146247, 661.8131002051039, 376.81004795398155, 16.727997213385265], "id_box_line": 24, "box_line": [157.46188092163828, 661.8131002051039, 327.730813646968, 16.727997213385265], "text": "Correcting the sentences", "category": "Section-header", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLEN+ZurichCnBT-Bold", "size": 1}}, {"id_box": 689001, "box": [108.3826466146247, 154.4576812689304, 808.4169563171332, 32.443315706410544], "id_box_line": 2, "box_line": [108.3826466146247, 154.4576812689304, 808.4169563171332, 14.221142843441953], "text": "\u2018Where were you at ten o\u2019clock yesterday morning?\u2019 the policeman", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689001, "box": [108.3826466146247, 154.4576812689304, 808.4169563171332, 32.443315706410544], "id_box_line": 3, "box_line": [108.3826466146247, 172.67990408316768, 75.15391710062538, 14.22109289217326], "text": "asked.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689002, "box": [130.23766095823447, 191.02199329181258, 715.6970374525301, 32.563198751240975], "id_box_line": 4, "box_line": [130.23766095823447, 191.02199329181258, 715.6970374525301, 14.221109542596196], "text": "John thought for a moment and then said, \u2018I was shopping.\u2019", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689002, "box": [130.23766095823447, 191.02199329181258, 715.6970374525301, 32.563198751240975], "id_box_line": 5, "box_line": [130.23766095823447, 209.36408250045747, 110.31314225933255, 14.221109542596082], "text": "\u2018Where?\u2019", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689003, "box": [130.23766095823447, 227.70617170910236, 230.73427581539556, 14.221109542596082], "id_box_line": 6, "box_line": [130.23766095823447, 227.70617170910236, 230.73427581539556, 14.221109542596082], "text": "\u2018I can\u2019t remember.\u2019", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689004, "box": [130.23766095823447, 245.92839452333953, 409.50832902476697, 14.221109542596196], "id_box_line": 7, "box_line": [130.23766095823447, 245.92839452333953, 409.50832902476697, 14.221109542596196], "text": "\u2018Try to remember. It\u2019s important.\u2019", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689005, "box": [130.23766095823447, 264.27048373198454, 786.4527276253473, 14.221109542596082], "id_box_line": 8, "box_line": [130.23766095823447, 264.27048373198454, 786.4527276253473, 14.221109542596082], "text": "John sighed and fidgeted. He wished his mother would come in.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689006, "box": [108.3826466146247, 282.61258959105226, 620.081244597881, 14.221092892173147], "id_box_line": 9, "box_line": [108.3826466146247, 282.61258959105226, 620.081244597881, 14.221092892173147], "text": "Perhaps he should offer the policeman a cup of tea.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689007, "box": [130.23766095823447, 300.95467879969715, 427.2380855318078, 14.22109289217326], "id_box_line": 10, "box_line": [130.23766095823447, 300.95467879969715, 427.2380855318078, 14.22109289217326], "text": "\u2018Would you like a drink?\u2019 he asked.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689008, "box": [130.23766095823447, 319.29676800834204, 644.1491031197911, 14.22109289217326], "id_box_line": 11, "box_line": [130.23766095823447, 319.29676800834204, 644.1491031197911, 14.22109289217326], "text": "\u2018Not while I\u2019m on duty,\u2019 the policeman replied coldly.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689009, "box": [108.3826466146247, 462.43720678808336, 758.1776580098809, 14.22109289217326], "id_box_line": 15, "box_line": [108.3826466146247, 462.43720678808336, 758.1776580098809, 14.22109289217326], "text": "ladies, companies, monkeys, tomatoes, boas, princesses, dances", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689010, "box": [108.3826466146247, 530.7704923902045, 769.5422335359582, 14.221109542596139], "id_box_line": 18, "box_line": [108.3826466146247, 530.7704923902045, 769.5422335359582, 14.221109542596139], "text": "dining, living, hitting, hopping, skipping, writing, mating, mining", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689011, "box": [108.3826466146247, 598.9839615491865, 808.5534894294448, 32.563232052086846], "id_box_line": 21, "box_line": [108.3826466146247, 598.9839615491865, 808.5534894294448, 14.221126193019018], "text": "happily, joyfully, kindly, angrily, wonderfully, clearly, quickly, care-", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689011, "box": [108.3826466146247, 598.9839615491865, 808.5534894294448, 32.563232052086846], "id_box_line": 22, "box_line": [108.3826466146247, 617.3260674082543, 63.76199852706701, 14.221126193019074], "text": "lessly", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689012, "box": [108.3826466146247, 685.659402961644, 310.17724495608996, 14.221126193019074], "id_box_line": 25, "box_line": [108.3826466146247, 685.659402961644, 36.58236885756837, 14.221126193019074], "text": "(a)", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689012, "box": [108.3826466146247, 685.659402961644, 310.17724495608996, 14.221126193019074], "id_box_line": 26, "box_line": [152.12118394992845, 685.659402961644, 266.4387076207862, 14.221126193019074], "text": "I know you are there.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689013, "box": [108.38263447295176, 703.8816257758813, 414.42569139201885, 14.221126193019074], "id_box_line": 27, "box_line": [108.38263447295176, 703.8816257758813, 38.0458320168749, 14.221126193019074], "text": "(b)", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689013, "box": [108.38263447295176, 703.8816257758813, 414.42569139201885, 14.221126193019074], "id_box_line": 28, "box_line": [153.55847573319343, 703.8816257758813, 369.2498501317772, 14.221126193019074], "text": "I can see two ships on the sea.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689014, "box": [108.38263447295176, 722.223731634949, 527.7712134349448, 14.221126193019018], "id_box_line": 29, "box_line": [108.38263447295176, 722.223731634949, 34.59031796642731, 14.221126193019018], "text": "(c)", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689014, "box": [108.38263447295176, 722.223731634949, 527.7712134349448, 14.221126193019018], "id_box_line": 30, "box_line": [150.04951403842438, 722.223731634949, 486.10433386947216, 14.221126193019018], "text": "Did you know their house is to be sold?", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689015, "box": [108.38263447295176, 740.5658208435939, 375.25052291372083, 14.221126193019074], "id_box_line": 31, "box_line": [108.38263447295176, 740.5658208435939, 37.848496441853115, 14.221126193019074], "text": "(d)", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689015, "box": [108.38263447295176, 740.5658208435939, 375.25052291372083, 14.221126193019074], "id_box_line": 32, "box_line": [153.32416876404196, 740.5658208435939, 330.30898862263064, 14.221126193019074], "text": "Here is your packed lunch.", "category": "Text", "words": [], "linking": [], "font": {"color": [0, 0, 0, 255], "name": "/FGPLCM+TimesTen-Roman", "size": 1}}, {"id_box": 689016, "box": [108.3826466146247, 758.7880436578312, 409.0437916531565, 14.22